{%- comment -%} VERSION: 1.0.0 Please do not delete this line. {%- endcomment -%}
{%- capture bc_layout -%}
{%- case layout -%}
{%- when 'productGridItemHtml' -%}
{% raw %}
<product-card class="{{gridWidthClass}}">
  {{itemTagLabels}}
  <div class="product-card__figure">
    {{itemImages}}

    <button type="button" aria-controls="quick-buy-{{itemId}}" aria-expanded="false" is="custom-button" aria-busy="false" class="product-card__quickview">
      <span class="quickview__text"><span>Quick view</span></span>
      <span class="quickview__icon">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="17" fill="none" viewBox="0 0 16 17">
          <path fill="#6B6B6B" fill-rule="evenodd" d="M1.5 8.5c0-.16.05-.316.144-.448.485-.675 1.319-1.698 2.42-2.548C5.164 4.653 6.5 4 8 4s2.835.653 3.937 1.504c1.1.85 1.934 1.873 2.42 2.548a.774.774 0 0 1 0 .896c-.486.675-1.32 1.698-2.42 2.548C10.835 12.347 9.5 13 8 13s-2.835-.653-3.937-1.504c-1.1-.85-1.934-1.873-2.42-2.548A.774.774 0 0 1 1.5 8.5ZM8 3c-1.797 0-3.343.782-4.548 1.713-1.206.931-2.105 2.038-2.62 2.756L.83 7.47a1.774 1.774 0 0 0 0 2.058l.001.002c.516.718 1.415 1.825 2.62 2.756C4.658 13.218 6.204 14 8 14c1.797 0 3.343-.782 4.548-1.713 1.206-.931 2.105-2.038 2.62-2.756l.002-.002a1.774 1.774 0 0 0 0-2.058l-.001-.002c-.516-.718-1.415-1.825-2.62-2.756C11.342 3.782 9.796 3 8 3ZM5.5 8.5a2.5 2.5 0 1 1 5 0 2.5 2.5 0 0 1-5 0ZM8 5a3.5 3.5 0 1 0 0 7 3.5 3.5 0 0 0 0-7Z" clip-rule="evenodd"/>
        </svg>
      </span>
    </button>
  </div>
  <div class="product-card__info">
    {{itemVendor}}
    <div class="h-stack wrap align-start gap-2">
      <span class="product-card__title">
        <a href="{{itemUrl}}" class="bold">{{itemTitle}}</a>
      </span>
      {{itemRegions}}
      {{itemPrice}}
    </div>

    <div class="product-card__select-format">
      <a href="{{itemUrl}}" class="button product-card__select-format__link">
        <span class="select-format-text">
          <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" viewBox="0 0 20 20"><path fill="currentColor" fill-rule="evenodd" d="M8.013 5.07c0-1.19.955-2.153 2.133-2.153s2.133.964 2.133 2.152v.07H8.013v-.07ZM6.774 6.388v1.458c0 .345.278.625.62.625.342 0 .619-.28.619-.625V6.39h4.266v1.458c0 .345.277.625.62.625.341 0 .619-.28.619-.625V6.39h2.201v4.236c0 .345.277.625.62.625.342 0 .619-.28.619-.625V5.764a.622.622 0 0 0-.62-.625h-2.82v-.07c0-1.879-1.51-3.402-3.372-3.402-1.862 0-3.372 1.523-3.372 3.402v.07h-2.82c-.343 0-.62.28-.62.625v11.805c0 .346.277.625.62.625h7.568c.342 0 .62-.28.62-.625a.622.622 0 0 0-.62-.625h-6.95V6.39h2.202Zm8.877 6.389c.341 0 .619.28.619.625v1.458h1.445c.342 0 .619.28.619.625a.622.622 0 0 1-.62.625H16.27v1.458a.622.622 0 0 1-.62.625.622.622 0 0 1-.619-.625v-1.458h-1.445a.622.622 0 0 1-.619-.625c0-.345.277-.625.62-.625h1.444v-1.458c0-.346.277-.625.62-.625Z" clip-rule="evenodd"/></svg> <span>Select Format</span>
        </span>
      </a>
    </div>
  </div>
  <quick-buy-drawer id="quick-buy-{{itemId}}" header-bordered open-from="left" handle="{{itemHandle}}" role="region" aria-live="polite" class="quick-buy-drawer drawer"></quick-buy-drawer>
</product-card>
{% endraw %}
{%- when 'productListItemHtml' -%}
{% raw %}
<div class="boost-pfs-filter-product-item boost-pfs-filter-product-item-list {{soldOutClass}}{{saleClass}} {{itemActiveSwapClass}}">
  <div class="boost-pfs-filter-product-item-inner">
    <div class="boost-pfs-filter-product-item-image">
        {{itemImages}}
        <div class="boost-pfs-filter-product-item-label">{{itemLabels}}{{itemTagLabels}}</div>
    </div>
    <div class="boost-pfs-filter-product-bottom">
      <div class="boost-pfs-filter-product-bottom-inner">
        <a href="{{itemUrl}}" class="boost-pfs-filter-product-item-title">{{itemTitle}}</a>
        {{itemVendor}}
        {{itemReviews}}
        {{itemPrice}}
        <p class="boost-pfs-filter-des">{{itemDescription}}</p>
        {{itemSwatches}}
      </div>
    </div>
  </div>
</div>
{% endraw %}
{%- when 'productListPlaceholderHtml' -%}
{% raw %}
<div class="boost-pfs-filter-product-item boost-pfs-filter-product-item-list {{class.filterProductSkeleton}}">
  <div class="boost-pfs-filter-product-item-image">
    <div class="{{class.filterProductSkeleton}}-image" style="padding-top: {{paddingTop}}%"></div>
  </div>
  <div class="boost-pfs-filter-product-bottom">
    <div class="boost-pfs-filter-product-bottom-inner">
    <a href="#" class="boost-pfs-filter-product-item-title"><span class="boost-pfs-filter-skeleton-text boost-pfs-filter-skeleton-width4"></span></a>
    <p class="boost-pfs-filter-product-item-vendor"><span class="boost-pfs-filter-skeleton-text boost-pfs-filter-skeleton-width1"></span></p>
    <p class="boost-pfs-filter-product-item-price"><span class="boost-pfs-filter-skeleton-text boost-pfs-filter-skeleton-width2"></span></p>
    <p class="boost-pfs-filter-des">
      <span class="boost-pfs-filter-skeleton-text" style="width: 100%"></span><br />
      <span class="boost-pfs-filter-skeleton-text" style="width: 100%"></span><br />
      <span class="boost-pfs-filter-skeleton-text" style="width: 100%"></span><br />
      <span class="boost-pfs-filter-skeleton-text" style="width: 30%"></span>
    </p>
    </div>
  </div>
</div>
{% endraw %}

{%- when 'soldOutLabelHtml' -%}

<span class="soldout boost-pfs-filter-label boost-pfs-filter-label-display-{{- sold_out_display -}}">
{% if sold_out_display == 'image' %}
<img src="{{sold_out_img}}" alt="Sold out" />
{% else %}
{{ label_soldout | default: 'Sold Out' }}
{% endif %}
</span>

{%- when 'saleLabelHtml' -%}
<span class="sale boost-pfs-filter-label boost-pfs-filter-label-display-{{- sale_label_display -}}">
  {% if sale_label_display == 'image' %}
  <img src="{{sale_label_img}}" alt="Sale" />
  {% else %}
    {% if sale_label_display == 'sale_percent' %}
        {{label_sale_percent}}
    {% else %}
    {{- label_sale | default: 'Sale' -}}
    {% endif %}
  {% endif %}
</span>


{%- when 'tagLabelHtml' -%}
{% raw %}
<span class="tag boost-pfs-filter-label {{labelTag}}" >{{labelTag}}</span>
{% endraw %}

{%- when 'vendorHtml' -%}
{% raw %}
<p class="boost-pfs-filter-product-item-vendor">{{itemVendorLabel}}</p>
{% endraw %}

{%- when 'previousActiveHtml' -%}
{% raw %}
<li><a href="{{itemUrl}}" aria-label="Page Previous">
  <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" viewBox="0 0 20 20">
    <path fill="#383838" fill-rule="evenodd" d="M12.942 4.558a.625.625 0 0 1 0 .884L8.384 10l4.558 4.558a.625.625 0 1 1-.884.884l-5-5a.625.625 0 0 1 0-.884l5-5a.625.625 0 0 1 .884 0Z" clip-rule="evenodd"/>
  </svg>
</a></li>
{% endraw %}

{%- when 'previousDisabledHtml' -%}
<li class="boost-pfs-filter-pagination-disabled"><span aria-label="Page Previous">
  <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" viewBox="0 0 20 20">
    <path fill="#CCCCCC" fill-rule="evenodd" d="M12.942 4.558a.625.625 0 0 1 0 .884L8.384 10l4.558 4.558a.625.625 0 1 1-.884.884l-5-5a.625.625 0 0 1 0-.884l5-5a.625.625 0 0 1 .884 0Z" clip-rule="evenodd"/>
  </svg>
</span></li>

{%- when 'nextActiveHtml' -%}
{% raw %}
<li><a href="{{itemUrl}}" aria-label="Page Next">
  <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" viewBox="0 0 20 20">
    <path fill="#383838" fill-rule="evenodd" d="M7.058 4.558a.625.625 0 0 1 .884 0l5 5a.625.625 0 0 1 0 .884l-5 5a.625.625 0 1 1-.884-.884L11.616 10 7.058 5.442a.625.625 0 0 1 0-.884Z" clip-rule="evenodd"/>
  </svg>
</a></li>
{% endraw %}

{%- when 'nextDisabledHtml' -%}
<li class="boost-pfs-filter-pagination-disabled"><span aria-label="Page Next">
  <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" viewBox="0 0 20 20">
    <path fill="#CCCCCC" fill-rule="evenodd" d="M7.058 4.558a.625.625 0 0 1 .884 0l5 5a.625.625 0 0 1 0 .884l-5 5a.625.625 0 1 1-.884-.884L11.616 10 7.058 5.442a.625.625 0 0 1 0-.884Z" clip-rule="evenodd"/>
  </svg>
</span></li>

{%- when 'pageItemHtml' -%}
{% raw %}
<li><a href="{{itemUrl}}" aria-label="Page {{itemTitle}}" title="Page {{itemTitle}}">{{itemTitle}}</a></li>
{% endraw %}

{%- when 'pageItemSelectedHtml' -%}
{% raw %}
<li><span class="boost-pfs-filter-pagination-active" aria-label="Page {{itemTitle}}">{{itemTitle}}</span></li>
{% endraw %}

{% when 'pageItemRemainHtml' %}
{% raw %}
<li><span aria-label="Page {{itemTitle}}">{{itemTitle}}</span></li>
{% endraw %}

{%- when 'paginateHtml' -%}
{% raw %}
<ul>{{previous}}{{pageItems}}{{next}}</ul>
{% endraw %}

{%- when 'sortingHtml' -%}
{% raw %}
<label>{{sortingLabel}}:</label>
{% endraw %}
<button class="boost-pfs-filter-top-sorting-wrapper" aria-label="{%- assign temp = 'collections.sorting.title' | t  -%} {%- unless temp contains 'translation missing'  -%} {{- temp -}} {%- else -%} Sort By {%- endunless -%}"><span aria-hidden="true"><span aria-hidden="true"> {%- assign temp = 'collections.sorting.title' | t  -%} {%- unless temp contains 'translation missing'  -%} {{- temp -}} {%- else -%} Sort By {%- endunless -%}</span></span></button>
{% raw %}
<ul class="boost-pfs-filter-filter-dropdown">{{sortingItems}}</ul>
{% endraw %}

{%- when 'showLimitHtml' -%}
{% raw %}
<label> Show </label><select class="boost-pfs-filter-filter-dropdown">{{showLimitItems}}</select>
{% endraw %}

{%- when 'breadcrumbHtml' -%}
{% raw %}
<a href="/"> Home </a> {{breadcrumbDivider}} {{breadcrumbItems}}
{% endraw %}

{%- when 'breadcrumbDivider' -%}
<span class="divider">/</span>

{%- when 'breadcrumbItemLink' -%}
{% raw %}
<a href="{{itemLink}}">{{itemTitle}}</a>
{% endraw %}

{%- when 'breadcrumbItemSelected' -%}
{% raw %}
<span>{{itemTitle}}</span>
{% endraw %}

{%- endcase -%}
{%- endcapture -%}

{%- assign str = '}}' -%}
{{- bc_layout | json | replace: '[[', '{{' | replace: ']]', str -}}